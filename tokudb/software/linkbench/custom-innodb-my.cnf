[mysqld]

default_tmp_storage_engine=MyISAM
eq_range_index_dive_limit=1000
key_buffer_size=200M
lock_wait_timeout=60
max_connections=2000
metadata_locks_hash_instances=256 
query_cache_size=0
query_cache_type=0
skip_log_bin
sql_mode=NO_ENGINE_SUBSTITUTION,STRICT_TRANS_TABLES 
table-definition-cache=1000
table-open-cache=2000
table-open-cache-instances=1

innodb_adaptive_flushing=1
innodb_adaptive_flushing_lwm=10
innodb_buffer_pool_instances=8
innodb_buffer_pool_size=60G
innodb_checksum_algorithm=CRC32
innodb_compression_failure_threshold_pct=10
innodb_compression_level=1
innodb_compression_pad_pct_max=37
innodb_deadlock_detect=0
innodb_doublewrite=0
innodb_file_format=barracuda
innodb_file_per_table
innodb_flushing_avg_loops=30
innodb_flush_log_at_trx_commit=2
innodb_flush_log_at_trx_commit=2
innodb_flush_method=O_DIRECT
innodb_flush_neighbors=2
innodb_histogram_step_size_async_read      = 128us
innodb_histogram_step_size_async_write     = 128us
innodb_histogram_step_size_double_write    = 128us 
innodb_histogram_step_size_file_flush_time = 128us
innodb_histogram_step_size_fsync           = 128us
innodb_histogram_step_size_log_write       = 128us
innodb_histogram_step_size_sync_read       = 128us
innodb_histogram_step_size_sync_write      = 128us
innodb_io_capacity=3000
innodb_lock_wait_timeout=2
innodb_log_buffer_size=4M
innodb_log_compressed_pages=0
innodb_log_file_size=3900M
innodb_lru_scan_depth=500
innodb_max_dirty_pages_pct=80
innodb_max_dirty_pages_pct_lwm=0
innodb_old_blocks_pct=50
innodb_old_blocks_time=1000
innodb_page_size=8k
innodb_purge_threads=4
innodb_read_io_threads=8
innodb_stats_on_metadata=0
innodb_thread_concurrency=0
innodb_thread_concurrency=0
innodb_write_io_threads=32
innodb_zlib_wrap=0
